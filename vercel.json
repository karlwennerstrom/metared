{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["backend/**"],
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.js"
    }
  ],
  "functions": {
    "backend/api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}